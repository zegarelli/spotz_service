{
  "name": "objection-knex",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "migrate": "knex migrate:latest",
    "migrate-test": "export DB_NAME=spotz_test && knex migrate:latest",
    "start": "node ./bin/www",
    "seed": "knex seed:run",
    "seed-test": "export DB_NAME=spotz_test && knex seed:run",
    "setup-db": "npm run migrate && npm run seed",
    "setup-test-db": "npm run migrate-test && npm run seed-test",
    "test": "export DB_NAME=spotz_test && jest --detectOpenHandles --collectCoverage",
    "test-watch": "export DB_NAME=spotz_test && jest --watch"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "knex": "^0.19.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "objection": "^1.6.9",
    "pg": "^7.12.0"
  }
}
