{
  "name": "objection-knex",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www",
    "migrate": "knex migrate:latest",
    "migrate-test": "export DB_NAME=spotz_test && knex migrate:latest",
    "start": "node ./bin/www",
    "seed": "knex seed:run",
    "seed-test": "export DB_NAME=spotz_test && knex seed:run",
    "setup-db": "npm run migrate && npm run seed",
    "setup-test-db": "npm run migrate-test && npm run seed-test",
    "test": "export NODE_ENV=test && nyc node_modules/mocha/bin/_mocha --exit",
    "test-watch": "export DB_NAME=spotz_test && jest --watch --collectCoverage"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3.1.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "knex": "^0.19.1",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "objection": "2.1.3",
    "pg": "^7.12.0"
  },
  "devDependencies": {
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "chai-http": "^4.3.0",
    "danger": "^9.3.0",
    "dirty-chai": "~2.0.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "sinon": "^7.5.0",
    "sinon-chai": "~3.3.0",
    "standard": "^14.3.3"
  }
}
